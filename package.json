{
  "name": "decree",
  "version": "0.0.2",
  "description": "Declarative arguments-resolver",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive --reporter spec tests",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive --reporter spec tests",
    "travis": "./node_modules/.bin/istanbul cover --report lcovonly ./node_modules/.bin/_mocha -- --recursive --reporter spec --bail tests && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EyalAr/Decree.git"
  },
  "keywords": [
    "declarative",
    "arguments",
    "resolve",
    "disambiguate",
    "match"
  ],
  "author": "Eyal Arubas <eyalarubas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EyalAr/Decree/issues"
  },
  "homepage": "https://github.com/EyalAr/Decree",
  "devDependencies": {
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.2",
    "mkdirp": "^0.5.0",
    "mocha": "^1.20.1",
    "mocha-lcov-reporter": "0.0.1",
    "should": "^4.0.4"
  },
  "dependencies": {
    "clone": "^0.1.18"
  }
}
